{
  "name": "sports-analytics-pro",
  "version": "1.0.0",
  "description": "Enterprise-level sports analytics platform with advanced insights and AI-powered predictions",
  "main": "dist/api.js",
  "scripts": {
    "start": "node dist/api.js",
    "dev": "nodemon --exec ts-node src/api.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "build:prod": "npm run clean && npm run build",
    "start:prod": "NODE_ENV=production node dist/api.js",
    "docker:build": "docker build -t sports-analytics-pro .",
    "docker:run": "docker run -p 3000:3000 sports-analytics-pro",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "migrate": "typeorm migration:run",
    "migrate:revert": "typeorm migration:revert",
    "seed": "ts-node src/database/seeds/index.ts",
    "coverage": "jest --coverage",
    "security-check": "npm audit",
    "update-deps": "npm update",
    "check-updates": "ncu",
    "verify": "node scripts/verify_connections.js",
    "init:db": "node scripts/initialize_mongodb.js",
    "prepare:prod": "npm run init:db && npm run verify",
    "test:prediction": "node scripts/test_prediction_system.js",
    "calibrate:models": "node scripts/run_model_calibration.js",
    "correlation:rebuild": "node scripts/rebuild_correlations.js"
  },
  "keywords": [
    "sports",
    "analytics",
    "ai",
    "predictions",
    "enterprise"
  ],
  "author": "Sports Analytics Pro Team",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "chart.js": "^4.4.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "cron": "^4.1.3",
    "d3": "^7.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^7.1.0",
    "graphql": "^15.10.1",
    "helmet": "^7.2.0",
    "html2canvas": "^1.4.1",
    "idb": "^7.1.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "mathjs": "^12.4.3",
    "mongoose": "^7.8.6",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.0",
    "redis": "^4.7.0",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mathjs": "^9.4.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.19",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/sports-analytics-pro.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/sports-analytics-pro/issues"
  },
  "homepage": "https://github.com/your-org/sports-analytics-pro#readme",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
