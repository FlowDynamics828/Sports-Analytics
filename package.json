{
  "name": "sports-analytics",
  "version": "5.0.0",
  "description": "Enterprise-Grade Professional Sports Analytics Platform",
  "main": "api.js",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=11.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlowDynamics828/Sports-Analytics.git"
  },
  "scripts": {
    "start": "node startup.js",
    "dev": "nodemon startup.js",
    "dev:debug": "nodemon --inspect startup.js",
    "prod": "NODE_ENV=production node startup.js",
    "prod:cluster": "NODE_ENV=production node startup.js --cluster",
    "start:direct": "node api.js",
    "dev:direct": "nodemon api.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:integration": "jest --config jest.integration.config.js",
    "test:performance": "artillery run performance-tests/",
    "test:mongodb": "node test-mongodb-connection.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "security:audit": "npm audit",
    "prepare": "husky install",
    "db:seed": "node scripts/enhanced-seeder.js",
    "db:migrate": "node scripts/migrations/run.js",
    "monitoring:start": "node scripts/monitoring/start.js",
    "analytics:train": "node scripts/analytics/train-models.js",
    "docs:generate": "jsdoc -c jsdoc.json",
    "verify:python": "node scripts/verify-python-env.js",
    "python": "node scripts/verify-python-env.js",
    "check:python": "node scripts/check-python-path.js",
    "fix:python": "node scripts/fix-python-timeout.js",
    "fix:python-timeout": "node scripts/fix-python-timeout.js",
    "fix:redis": "node scripts/fix-redis-connection.js",
    "fix:memory": "node scripts/fix-memory-issues.js",
    "fix:maxlisteners": "node scripts/fix-max-listeners.js",
    "fix:all": "node scripts/fix-all-issues.js",
    "verify:all": "node scripts/verify-python-env.js && node test-mongodb-connection.js",
    "diagnose": "node scripts/quick-diagnose.js",
    "diagnose:full": "node scripts/diagnose-system-fixed.js",
    "fix": "node scripts/fix-all-issues.js",
    "fix:quick": "node scripts/fix-python-path.js",
    "setup": "node scripts/fix-all-issues.js && npm run start:optimized",
    "optimize:memory": "node scripts/memoryManager.js",
    "start:optimized": "node --max-old-space-size=4096 --expose-gc startup.js",
    "fix:python-verification": "node scripts/fix-python-timeout.js"
  },
  "dependencies": {
    "@elastic/elasticsearch": "8.11.0",
    "@google-cloud/storage": "7.7.0",
    "@sentry/node": "7.91.0",
    "amqplib": "0.10.3",
    "async-lock": "^1.4.1",
    "aws-sdk": "2.1531.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.3",
    "bull": "4.12.0",
    "cache-manager": "^5.7.6",
    "cache-manager-ioredis": "^2.1.0",
    "cluster": "^0.7.7",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "connect-redis": "7.1.0",
    "cookie": "0.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "4.2.0",
    "datadog-metrics": "0.11.0",
    "date-fns": "3.0.6",
    "dotenv": "^16.4.7",
    "events": "^3.3.0",
    "express": "^4.21.2",
    "express-async-handler": "1.2.0",
    "express-mongo-sanitize": "2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "fast-csv": "5.0.0",
    "helmet": "^7.1.0",
    "helmet-csp": "3.4.0",
    "ioredis": "^5.5.0",
    "joi": "17.11.0",
    "jsonwebtoken": "9.0.2",
    "kafkajs": "2.2.4",
    "lodash": "^4.17.21",
    "mathjs": "12.3.0",
    "ml-matrix": "6.11.0",
    "ml-regression": "5.0.0",
    "mongodb": "^6.14.0",
    "mongoose": "^8.10.1",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-schedule": "2.1.1",
    "nodemailer": "6.10.0",
    "opossum": "^8.4.0",
    "os": "^0.1.2",
    "papaparse": "5.4.1",
    "path-to-regexp": "6.2.1",
    "perf_hooks": "^0.0.1",
    "pino": "8.16.2",
    "prom-client": "^15.1.3",
    "python-shell": "^5.0.0",
    "rate-limit-mongo": "2.3.2",
    "redis": "4.6.11",
    "safe-buffer": "5.2.1",
    "scikit-learn": "0.1.0",
    "send": "0.19.0",
    "serialize-javascript": "6.0.2",
    "serve-static": "1.16.1",
    "sharp": "0.33.1",
    "socket.io": "4.8.1",
    "stripe": "14.9.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0",
    "uuid": "9.0.1",
    "v8": "^0.1.0",
    "validator": "13.11.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.18.1",
    "xlsx": "0.18.5",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@babel/cli": "7.23.4",
    "@babel/core": "7.23.6",
    "@babel/helper-compilation-targets": "7.23.6",
    "@babel/plugin-transform-runtime": "7.23.6",
    "@babel/preset-env": "7.23.6",
    "@babel/runtime": "7.23.6",
    "artillery": "2.0.3",
    "chai": "4.3.10",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.1",
    "eslint-plugin-security": "1.7.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-extended": "4.0.2",
    "jsdoc": "4.0.2",
    "lint-staged": "15.4.3",
    "micromatch": "4.0.8",
    "mocha": "10.2.0",
    "nanoid": "3.3.6",
    "nodemon": "3.0.2",
    "nyc": "15.1.0",
    "prettier": "3.1.1",
    "sinon": "17.0.1",
    "supertest": "6.3.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFiles": [
      "./jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/docs/**",
      "!src/tests/**"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "*.test.js",
      "docs/*",
      "node_modules/**/node_modules"
    ],
    "delay": "2500"
  },
  "keywords": [
    "sports",
    "analytics",
    "machine-learning",
    "real-time",
    "predictions",
    "statistics",
    "api"
  ],
  "author": "FlowDynamics828",
  "license": "UNLICENSED"
}